generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
  output          = "./generate/mongoclient"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_DB_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
}
